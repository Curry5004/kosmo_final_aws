<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
 	
	<!-- Alias 설정 -->
	<typeAliases>
		<typeAlias alias="user" type="com.springbook.biz.user.UserVO" />
		<typeAlias alias="board" type="com.springbook.biz.board.BoardVO"/>
		<typeAlias alias="category" type="com.springbook.biz.main.CategoryVO" />
		<typeAlias alias="mbti" type="com.springbook.biz.main.MbtiVO" />
		<typeAlias alias="party" type="com.springbook.biz.party.PartyVO" />
		<typeAlias alias="member" type="com.springbook.biz.memberList.MemberListVO" />
		<typeAlias alias="schedule" type="com.springbook.biz.sch.SchVO"/>
		<typeAlias alias="album" type="com.springbook.biz.album.AlbumVO" />
		<typeAlias alias="albumComment" type="com.springbook.biz.albumComment.AlbumCommentVO" />
		<typeAlias alias="boardComment" type="com.springbook.biz.boardComment.BoardCommentVO"/>
		<typeAlias alias="reportBoard" type="com.springbook.biz.report.ReportBoardVO" />
		<typeAlias alias="qna" type="com.springbook.biz.report.QnaVO" />
		<typeAlias alias="chat" type="com.springbook.biz.chat.ChatVO" />
	</typeAliases>

	<!-- Sql Mapper 설정 -->
	<mappers>
		<mapper resource="mappings/user-mapping.xml" ></mapper>
		<mapper resource="mappings/category-mapping.xml"></mapper>
		<mapper resource="mappings/mbti-mapping.xml"></mapper>
		<mapper resource="mappings/party-mapping.xml"></mapper>
		<mapper resource="mappings/memberList-mapping.xml"></mapper>
		<mapper resource="mappings/board-mapping.xml"></mapper>
		<mapper resource="mappings/schedule-mapping.xml"></mapper>
		<mapper resource="mappings/album-mapping.xml" />
		<mapper resource="mappings/albumComment-mapping.xml" />
		<mapper resource="mappings/mypage-mapping.xml" />		
		<mapper resource="mappings/boardComment-mapping.xml"></mapper>
		<mapper resource="mappings/admin-mapping.xml"></mapper>
		<mapper resource="mappings/report-mapping.xml" />
		<mapper resource="mappings/chat-mapping.xml" />
	</mappers>
	
</configuration>